<footer id="footer" class="clearfix">

	<div class="social-wrapper">
  	<% if (theme.social) { %>
      <% theme.social.forEach(function(value, key) {
           if (value.url && value.slug) { %>
        <a href="<%= value.url %>" class="social <%= value.slug %>"
          target="_blank" rel="external">
          <span class="icon icon-<%= value.slug %>"></span>
        </a>
      <% } }); %>
    <% } %>
  </div>

  <div>Linro powered by <a href='https://hexo.io' class="codename">Hexo</a> | <a href='https://github.com/stkevintan/hexo-theme-material-flow' class="codename">MaterialFlow</a></div>
  <div><a href="http://www.miitbeian.gov.cn/">苏ICP备17006152号</a></div>
</footer>
